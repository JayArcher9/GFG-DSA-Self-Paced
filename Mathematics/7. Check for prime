#include <iostream>
using namespace std;

int main() {

  int n;

  cout << "Enter a number: ";
  cin >> n;

  

  string res = "prime number";

  if(n == 2 || n == 3){
    res = "prime number";
  }

  else if(n%2 == 0 || n%3 == 0){
    res = "not a prime number";
  }

  for(int i = 5; i*i <=n; i += 6){
    if(n%i == 0 || n%(i+2) == 0){
      res = "not a prime number";
      break;
    }
  }
  
  cout << res;
