#include <iostream>
using namespace std;

int main() {

  long long n;
  cout<<"Enter a number: "<<endl;

  cin >> n;

  long long ans = 0;

  for(int i=5;i<=n;i=i*5){
    ans = ans + n/i;
    
  }

  long long sum = 1;


  for(int i = n; i>0; i--){
    sum *= i;
  }

  cout <<"Total number of trailing zeroes in "<< sum <<" is " << ans;
  
  
}
